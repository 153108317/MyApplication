<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.yh.myapplication">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"

        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <meta-data
                android:name="yh"
                android:value="${UMENG_CHANNEL_VALUE}" />
        </activity>
        <activity android:name=".activities.PicturesActivity"></activity>
        <activity android:name=".activities.WebViewActivity"></activity>
        <activity android:name=".activities.DetailActivity" />
        <activity android:name=".activities.OkHttpTestActivity"/>
        <activity android:name=".activities.RetrofitHttpTestActivity"/>
        <activity android:name=".activities.PagerSlidingTabStripActivity"/>
        <activity android:name=".activities.ItemRemoveRecyclerviewActivity"/>
        <activity android:name=".activities.ScrollerActivity"/>

    </application>

</manifest>